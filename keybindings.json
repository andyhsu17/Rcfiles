// Place your key bindings in this file to override the defaults
[

{ "key": "shift+tab",              "command": "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
                                     "when": "!activeEditorGroupEmpty" },

{ "key": "shift+tab",              "command": "workbench.action.quickOpenNavigateNextInEditorPicker",
                                     "when": "inEditorsPicker && inQuickOpen" },
{ "key": "ctrl+f",                "command": "actions.find",
                                     "when": "editorFocus || editorIsOpen" },
{ "key": "alt+l",          "command": "workbench.action.navigateForward",
                                     "when": "canNavigateForward" },
{ "key": "alt+h",            "command": "workbench.action.navigateBack",
                                     "when": "canNavigateBack" },
{ "key": "cmd+l",          "command": "workbench.action.navigateForward",
                                     "when": "canNavigateForward" },
{ "key": "cmd+h",            "command": "workbench.action.navigateBack",
                                     "when": "canNavigateBack" },

{
    "key": "ctrl+space",
    "command": "workbench.action.quickSwitchWindow"
},
{
    "key": "ctrl+shift+s",
    "command": "workbench.action.files.saveFiles"
},
{
    "key": "cmd+shift+s",
    "command": "workbench.action.files.saveFiles"
},
{
    "key": "ctrl+p",
    "command": "workbench.action.quickOpen"
},
{
    "key": "cmd+p",
    "command": "-workbench.action.quickOpen"
}
]
